<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/ontologies/Flavour#"
     xml:base="http://www.semanticweb.org/ontologies/Flavour"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
