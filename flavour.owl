    <owl:AnnotationProperty rdf:about="http://www.semanticweb.org/ontologies/Flavour#AnnotationProperty1">
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">synonym</rdfs:label>
    </owl:AnnotationProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
